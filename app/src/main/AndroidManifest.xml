        </activity>
        <activity android:name=".ui.activity_login" />
        <activity android:name=".ui.activity_register" />
        <activity android:name=".ui.activity_home" />
        <activity android:name=".ui.activity_setting" />
        <activity android:name=".ui.activity_table" />
